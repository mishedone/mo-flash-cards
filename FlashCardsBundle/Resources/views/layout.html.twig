{% extends 'base.html.twig' %}

{% block body %}
    {% block content %}{% endblock %}
{% endblock %}

{% block stylesheets %}
    {% stylesheets '@bootstrap_css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {% javascripts '@jquery_js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}